

<%= form_tag("/search", method: "get") do %>
  <%= label_tag(:q, "firstname") %>
  <%= text_field_tag(:q) %><br>
  <%= label_tag(:q, "lastname ") %>
  <%= text_field_tag(:q) %><br>
  <%= label_tag(:q, "contactno:") %>
  <%= telephone_field(:user, :phone) %><br>
  <%= label_tag(:q, "d.o.b:") %>
  <%= date_field(:user, :born_on) %><br>
  <%= label_tag(:q, "e_mail id:") %>
  <%= email_field(:user, :address) %><br>
  <%= label_tag(:subject, "subjectchoice:") %><br>
  <%= check_box_tag(:subject) %>
  <%= label_tag(:subject, "maths") %><br>
  <%= check_box_tag(:subject) %>
  <%= label_tag(:subject, "biology") %><br><br>
  <%= text_area_tag(:message,"i am select this subject because", size: "24x6") %><br><br>
  <%= label_tag(:gender, "gender:") %><br>
  <%= radio_button_tag(:gender, "child") %>
  <%= label_tag(:gender, "male") %>
  <%= radio_button_tag(:gender, "adult") %>
  <%= label_tag(:gender, "female") %><br><br>
  <%= submit_tag("Submit") %><br>
 

<% end %>

<%= color_field(:user, :favorite_color) %>
<%= time_field(:task, :started_at) %>
<%= number_field(:product, :price, in: 1.0..20.0, step: 0.5) %>
<%= range_field(:product, :discount, in: 1..100) %>
